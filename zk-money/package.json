  "scripts": {
    "start:dev": "yarn copy-wasm && PORT=8080 react-scripts start",
    "build": "yarn copy-wasm && yarn clean && npx react-scripts build && mv build dist",
    "copy-wasm": "cp ./node_modules/@aztec/barretenberg/wasm/barretenberg.wasm ./public/ && cp ./node_modules/@aztec/barretenberg/worker.js ./public/",
    "clean": "rm -rf ./dist",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.14",
